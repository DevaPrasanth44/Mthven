pipeline {
    agent any

    stages {

        stage('Checkout Code') {
            steps {
                git branch: 'main',
                    url: 'https://github.com/DevaPrasanth44/Mthven.git'
            }
        }

        stage('Run Project') {
            steps {
                sh '''
                echo "Running project..."
                ls -l
                '''
            }
        }
    }
}
